; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="StellarConstructor"
run/main_scene="res://objects/game.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

SC="*res://objects/sc.tscn"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="viewport"

[file_customization]

folder_colors={
"res://objects/build_grid/": "blue",
"res://objects/builds/": "green",
"res://objects/ui/": "red"
}

[global_group]

landing_pads="For querying landing pads"

[gui]

theme/custom="res://styles/my_theme.tres"
theme/custom_font="res://styles/Roboto-Regular.ttf"

[layer_names]

2d_physics/layer_1="build"
2d_physics/layer_2="item"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
environment/defaults/default_clear_color=Color(0.207843, 0.0588235, 0.231373, 1)
